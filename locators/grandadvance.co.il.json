{
  "supplier_prefix": "grandadvance.co.il",

  "default_image_url": {
    "attribute": "src",
    "by": "XPATH",
    "selector": "//div[@class = 'big_image']//img",
    "strategy_for_multiple_selectors": "find_first_match",
    "if_list": "first",
    "mandatory": true,
    "text_to_be_present_in_element": "",
    "event": false,
    "timeout_for_event": "presence_of_element_located",
    "timeout": 0,
    "locator_description": "URL главного изображения товара grandadvance"
  },
  "name": {
    "attribute": "innerText",
    "by": "XPATH",
    "selector": "//div[contains(@id,'_product_title')]//h1",
    "strategy_for_multiple_selectors": "find_first_match",
    "if_list": "last",
    "mandatory": true,
    "text_to_be_present_in_element": "",
    "event": null,
    "timeout_for_event": "presence_of_element_located",
    "timeout": 0,
    "locator_description": "PrestaShop: Название товара grandadvance"
  },
  "description_short": {
    "attribute": "innerText",
    "by": "XPATH",
    "selector": "//div[@class = 'common_specifications_holder']",
    "strategy_for_multiple_selectors": "find_first_match",
    "if_list": "all",
    "mandatory": true,
    "text_to_be_present_in_element": "",
    "event": null,
    "timeout_for_event": "presence_of_element_located",
    "timeout": 0,
    "locator_description": "PrestaShop: Краткое описание товара grandadvance"
  },
  "description": {
    "attribute": "innerText",
    "by": "XPATH",
    "selector": "//div[@id='productDescription']",
    "strategy_for_multiple_selectors": "find_first_match",
    "if_list": "first",
    "mandatory": true,
    "event": null,
    "timeout_for_event": "presence_of_element_located",
    "timeout": 0,
    "locator_description": "Полное описание товара (дополнительно) grandadvance"
  },
  "specification": {
    "attribute": "innerText",
    "by": "XPATH",
    "selector": "//div[@class = 'properties']",
    "strategy_for_multiple_selectors": "find_first_match",
    "if_list": "all",
    "mandatory": true,
    "text_to_be_present_in_element": "",
    "event": null,
    "timeout_for_event": "presence_of_element_located",
    "timeout": 0,
    "locator_description": "PrestaShop: Характеристики товара grandadvance"
  },

  "brand": {
    "attribute": "innerText",
    "by": "XPATH",
    "selector": "//span[contains(text(), 'Brand')]/parent::td/following-sibling::td/span[contains(@class, 'po-break-word')]",
    "strategy_for_multiple_selectors": "find_first_match",
    "if_list": "first",
    "mandatory": true,
    "event": null,
    "timeout_for_event": "presence_of_element_located",
    "timeout": 0,
    "locator_description": "PrestaShop: Производитель (бренд) товара grandadvance"
  },
  "summary": {
    "attribute": "innerHTML",
    "by": "XPATH",
    "selector": "//div[contains(@data-a-expander-name , 'product_overview')]//table",
    "strategy_for_multiple_selectors": "find_first_match",
    "if_list": "first",
    "mandatory": true,
    "event": null,
    "timeout_for_event": "presence_of_element_located",
    "timeout": 0,
    "locator_description": "Сводка/краткие характеристики grandadvance"
  }
}